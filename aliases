#!/bin/bash

# Extend PATH
export PATH=~/.local/bin:$PATH

alias kube="~/.local/bin/kubectl"
alias k="~/.local/bin/kubectl"

alias repos="~/Workspace/Repos"

export REPOS="~/Workspace/Repos"
export  LOGS="~/Workspace/Logs"

# Source external configurations
source ~/Workspace/Repos/naranja/dotfiles/functions

